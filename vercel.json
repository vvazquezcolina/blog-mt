{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "framework": null,
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/blog",
      "destination": "/blog/es"
    },
    {
      "source": "/blog/",
      "destination": "/blog/es"
    },
    {
      "source": "/",
      "destination": "/blog/es"
    }
  ],
  "routes": [
    {
      "src": "/blog/assets/(.*)",
      "dest": "/blog/assets/$1"
    }
  ],
  "headers": [
    {
      "source": "/blog/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/blog/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

